<%- include('partials/headers') %>
<form class = "loginContainer" action = "/student/login" , method="POST">
  <div class = "container">
    <div class="form-group">
      <label>Roll Number</label>
      <input type="text" class="form-control" name = "rollno" >
    </div>
    <div class="form-group">
      <label>Password</label>
      <input type="password" class="form-control" name = "password">
    </div>
    <button type="submit" class="btn btn-primary">Log In</button>
    <button type="submit" class="btn btn-primary"><a href="/student/register">Register</a></button>
    <button type="submit" class="btn btn-primary"><a href="/student/forgot_pass">Forgot password</a></button>
  </div>
  </form>
<%- include('partials/footer') %>